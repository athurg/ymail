<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkStatusIcon" id="status_icon">
    <property name="pixbuf">icon.png</property>
    <property name="has_tooltip">True</property>
    <property name="tooltip_text">&#x6CA1;&#x6709;&#x90AE;&#x4EF6;</property>
    <property name="tooltip_markup">&#x6CA1;&#x6709;&#x90AE;&#x4EF6;</property>
    <property name="title">&#x90AE;&#x4EF6;&#x68C0;&#x6D4B;</property>
    <signal name="button_press_event" handler="status_icon_button_press_cb"/>
  </object>
  <object class="GtkAboutDialog" id="aboutdialog">
    <property name="border_width">5</property>
    <property name="title" translatable="yes">&#x90AE;&#x4EF6;&#x5BA2;&#x6237;&#x7AEF;</property>
    <property name="window_position">center-on-parent</property>
    <property name="destroy_with_parent">True</property>
    <property name="type_hint">normal</property>
    <property name="has_separator">False</property>
    <property name="program_name">&#x90AE;&#x4EF6;&#x5BA2;&#x6237;&#x7AEF;</property>
    <property name="version">0.0.1</property>
    <property name="copyright" translatable="yes">0.0.1</property>
    <property name="comments" translatable="yes">&#x57FA;&#x4E8E;GTK&#x548C;fetchmail&#x7684;&#x90AE;&#x4EF6;&#x5BA2;&#x6237;&#x7AEF;</property>
    <property name="website">http://www.gooth.cn/</property>
    <property name="license" translatable="yes">GPL</property>
    <property name="authors">&#x51AF;&#x5EFA;&#x6CE2;</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <placeholder/>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area1">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
